{
  "name": "generator-alfresco-docker-compose",
  "version": "0.0.0",
  "description": "Alfresco Docker Compose Generator",
  "homepage": "https://github.com/Alfresco",
  "author": {
    "name": "Angel Borroy",
    "email": "angel.borroy@alfresco.com"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "alfresco",
    "docker-compose",
    "generator",
    "yeoman-generator"
  ],
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "yeoman-generator": "^4.12.0",
    "yosay": "^2.0.2"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": "error"
    },
    "plugins": [
      "prettier"
    ]
  },
  "scripts": {
    "pretest": "eslint .",
    "test": "jest"
  },
  "repository": "alfresco/generator-alfresco-docker-compose",
  "license": "LGPL-3.0"
}
